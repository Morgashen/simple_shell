#include <stdio.h>

/**
 * main - Entry point
 *
 * Description: Simple program to print "Programming is like 
 * building a multilingual puzzle"
 *
 * Return: Always 0 (Success)
 */
int main(void)
{
        char *str = "Programming is like building a multilingual puzzle";
        
        printf("%s\n", str);

        return (0); 
}
